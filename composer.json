{
    "name": "flashytime/container",
    "description": "A lightweight PHP dependency injection container.",
    "keywords": [
        "di",
        "dependency-injection",
        "container"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "flashytime",
            "email": "myflashytime@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.6",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5"
    },
    "autoload": {
        "psr-4": {
            "Flashytime\\Container\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Flashytime\\Container\\Tests\\": "tests/"
        }
    }
}
